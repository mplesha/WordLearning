@model IList<DataAccessLayer.Entities.Profile>

<div align="left"><span class="label label-primary">@Resources.Resources.txtMyStudents</span></div>
<table class="table table-bordered table-hover own-grid">
    <thead>
        <tr>
            <th>@Resources.Resources.txtName</th>
            <th>@Resources.Resources.txtLastName</th>
            <th>@Resources.Resources.txtEmail</th>
            <th>@Resources.Resources.txtPhoneNumber</th>
            <th>@Resources.Resources.txtViewCourses</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@Model[i].FirstName</td>
                <td>@Model[i].LastName</td>
                <td>@Model[i].Email</td>
                <td>@Model[i].PhoneNumber</td>
                <td>

                    <button onclick=" window.location.href = '@Url.Action("StudentCourses", "Teacher",
                                                                  new { studentId = Model[i].Id})' " class="btn btn-default btn-sm dropdown-toggle own-button" name="View courses" type="submit">
                        @Resources.Resources.txtViewCourses</button>

                </td>
            </tr>
        }
    </tbody>
</table>
@Html.Partial("_Paging")